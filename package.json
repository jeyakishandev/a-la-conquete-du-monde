{
  "name": "a-la-conquete-du-monde",
  "version": "1.0.0",
  "description": "Fullstack Travel Blog Application - React + Node.js + Express + Prisma",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd server && npm run dev",
    "dev:frontend": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd server && npm test",
    "test:frontend": "cd client && npm test",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "db:migrate": "cd server && npx prisma migrate dev",
    "db:seed": "cd server && npx prisma db seed",
    "db:studio": "cd server && npx prisma studio",
    "db:reset": "cd server && npx prisma migrate reset",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd server && npm run lint",
    "lint:frontend": "cd client && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd server && npm run format",
    "format:frontend": "cd client && npm run format"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "prisma",
    "travel",
    "blog",
    "fullstack",
    "docker",
    "typescript"
  ],
  "author": "Jeya Kishan Karunanithy <k.jeyakishan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeyakishandev/a-la-conquete-du-monde.git"
  },
  "bugs": {
    "url": "https://github.com/jeyakishandev/a-la-conquete-du-monde/issues"
  },
  "homepage": "https://github.com/jeyakishandev/a-la-conquete-du-monde#readme"
}